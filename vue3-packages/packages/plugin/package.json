{
  "name": "@vue3-lowcode/plugin",
  "version": "1.0.0",
  "description": "Vue3 LowCode Engine Plugin System - 插件系统模块",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@vue3-lowcode/types": "workspace:*",
    "@vue3-lowcode/utils": "workspace:*",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.3",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue3",
    "lowcode",
    "plugin",
    "plugin-system"
